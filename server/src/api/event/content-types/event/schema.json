{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files"
      ],
      "type": "media",
      "multiple": false
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "price": {
      "type": "string"
    },
    "startDate": {
      "type": "datetime"
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "blocks.heading",
        "blocks.paragraph"
      ]
    },
    "event_signups": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event-signup.event-signup",
      "mappedBy": "event"
    }
  }
}
